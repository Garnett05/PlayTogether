            <Label Text="{Binding Msg}"/>
            <!--<Label Text="Username" FontSize="10" HorizontalOptions="EndAndExpand" VerticalOptions="End" />-->
            <!--<Label Text="08:00" FontSize="10" HorizontalOptions="End" VerticalOptions="End" />-->
        </StackLayout>
    </Frame>    
</ViewCell>