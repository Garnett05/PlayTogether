             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="PlayTogether.GroupChat.IncomingVIewCell">
    <Frame BackgroundColor="LightBlue"
           CornerRadius="8"
           Padding="10"
           HasShadow="False"
           Margin="10, 10, 80, 10" >
